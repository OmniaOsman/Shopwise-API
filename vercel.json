{
    "builds": [
        {
            "src": "shopwise/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.10.7"
            }
        },
        {
            "functions": {
              "api/**/*.py": {
                "excludeFiles": "images/**"
              }
            }
          }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "shopwise/wsgi.py"
        }
    ]
}